{
  "version": 2,
  "builds": [
    { "src": "api/user-data.ts", "use": "@vercel/node" },
    {
      "src": "client/index.html",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist/public" }
    }
  ],
  "routes": [
    { "src": "/api/user-data", "dest": "/api/user-data.ts" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/dist/public/index.html" }
  ]
}
